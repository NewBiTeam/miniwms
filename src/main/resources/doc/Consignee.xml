<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="工作表1">
		<section startRow="0" endRow="1"></section>
		<loop startRow="2" endRow="2" items="consigneeVOs" var="consigneeVO"
			varType="ndm.miniwms.vo.ConsigneeVO">
			<section startRow="2" endRow="2">
				<mapping row="2" col="0">consigneeVO.name</mapping>
				<mapping row="2" col="1">consigneeVO.address</mapping>
				<mapping row="2" col="2">consigneeVO.description</mapping>
				<mapping row="2" col="3">consigneeVO.contactName</mapping>
				<mapping row="2" col="4">consigneeVO.contactTel</mapping>
				<mapping row="2" col="5">consigneeVO.contactFax</mapping>
				<mapping row="2" col="6">consigneeVO.contactEmail</mapping>
				<mapping row="2" col="7">consigneeVO.contactQq</mapping>
				<mapping row="2" col="8">consigneeVO.contactMsn</mapping>
				<mapping row="2" col="9">consigneeVO.contactDesc</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0"></cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>  