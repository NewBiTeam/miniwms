<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet idx="0">
		<section startRow="0" endRow="1"></section>
		<loop startRow="2" endRow="2" items="stockItemVos" var="stockItemVo"
			varType="ndm.miniwms.vo.StockItemVo">
			<section startRow="2" endRow="2">
				<mapping row="2" col="0">stockItemVo.SKU</mapping>
				<mapping row="2" col="1">stockItemVo.name</mapping>
				<mapping row="2" col="2">stockItemVo.barcode</mapping>
				<mapping row="2" col="3">stockItemVo.weight</mapping>
				<mapping row="2" col="4">stockItemVo.volume</mapping>
				<mapping row="2" col="5">stockItemVo.categoryName</mapping>
				<mapping row="2" col="6">stockItemVo.brandName</mapping>
				<mapping row="2" col="7">stockItemVo.UOM</mapping>
				<mapping row="2" col="8">stockItemVo.hazardFlagName</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0"></cellcheck>
					<cellcheck offset="1"></cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>  