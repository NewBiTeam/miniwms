<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="工作表1">
		<section startRow="0" endRow="1"></section>
		<loop startRow="2" endRow="2" items="categoryVOs" var="categoryVO"
			varType="ndm.miniwms.vo.CategoryVO">
			<section startRow="2" endRow="2">
				<mapping row="2" col="0">categoryVO.name</mapping>
				<mapping row="2" col="1">categoryVO.description</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0"></cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>  